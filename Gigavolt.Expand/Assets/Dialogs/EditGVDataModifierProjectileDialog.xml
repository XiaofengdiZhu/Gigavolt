<CanvasWidget xmlns="runtime-namespace:Game">
	<CanvasWidget Size="700, 300" ClampToBounds="true" HorizontalAlignment="Center" VerticalAlignment="Center">
		<BevelledRectangleWidget Style="{Styles/DialogArea}"/>
		<StackPanelWidget Direction="Vertical" HorizontalAlignment="Center" VerticalAlignment="Near" Margin="0, 15">
			<LabelWidget Font="{Fonts/Pericles}" Text="[EditGVDataModifierProjectileDialog:1]" HorizontalAlignment="Center"/>
			<CanvasWidget Size="0, Infinity"/>
			<GridPanelWidget Name="BinaryInputGrid" HorizontalAlignment="Center" VerticalAlignment="Near" ColumnsCount="14" RowsCount="1"/>
			<StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center">
				<LabelWidget Text="[EditGVDataModifierProjectileDialog:2]"/>
				<CanvasWidget Size="124, 34">
					<BevelledRectangleWidget Style="{Styles/Area}"/>
					<TextBoxWidget Name="EditGVDataModifierProjectileDialog.Data" Size="120, 30" MaximumLength="11" FontScale="1" TextureLinearFilter="false" Margin="2, 2"/>
				</CanvasWidget>
			</StackPanelWidget>
			<LabelWidget Margin="0, 10" Text="[EditGVDataModifierProjectileDialog:3]" HorizontalAlignment="Center"/>
			<CanvasWidget Size="0, Infinity"/>
			<StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center">
				<BevelledButtonWidget Name="EditGVDataModifierProjectileDialog.OK" Style="{Styles/ButtonStyle_160x60}" Text="[EditGVDataModifierProjectileDialog:4]" Margin="20, 0"/>
				<BevelledButtonWidget Name="EditGVDataModifierProjectileDialog.Cancel" Style="{Styles/ButtonStyle_160x60}" Margin="20, 0" Text="[EditGVDataModifierProjectileDialog:5]"/>
			</StackPanelWidget>
		</StackPanelWidget>
	</CanvasWidget>
</CanvasWidget>