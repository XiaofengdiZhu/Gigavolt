<Screen xmlns="runtime-namespace:Game">
  <RectangleWidget Size="Infinity, Infinity" OutlineThickness="0"/>
  <StackPanelWidget Direction="Horizontal">
    <CanvasWidget Style="{Widgets/TopBarContents}">
      <LabelWidget Name="TopBar.Label" Text="[GVOscilloscopeScreen:1]"/>
    </CanvasWidget>
    <StackPanelWidget Direction="Vertical">
      <StackPanelWidget Direction="Horizontal">
        <BevelledButtonWidget Name="GVOscilloscopeScreen.TopButton" Size="188, 60">
          <CanvasWidget Name="BevelledButton.Canvas" Margin="0, 0" HorizontalAlignment="Near">
            <BevelledRectangleWidget Name="BevelledButton.Rectangle" CenterColor="112, 112, 112"/>
            <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Near" VerticalAlignment="Center" IsHitTestVisible="false">
              <LabelWidget Name="GVOscilloscopeScreen.TopInput" VerticalAlignment="Center" Margin="10, 0" Text="[GVOscilloscopeScreen:2]"/>
            </StackPanelWidget>
          </CanvasWidget>
        </BevelledButtonWidget>
        <CanvasWidget Size="4,0"/>
        <BevelledButtonWidget Name="GVOscilloscopeScreen.RightButton" Size="188, 60">
          <CanvasWidget Name="BevelledButton.Canvas" Margin="0, 0" HorizontalAlignment="Near">
            <BevelledRectangleWidget Name="BevelledButton.Rectangle" CenterColor="112, 112, 112"/>
            <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Near" VerticalAlignment="Center" IsHitTestVisible="false">
              <LabelWidget Name="GVOscilloscopeScreen.RightInput" VerticalAlignment="Center" Margin="10, 0" Text="[GVOscilloscopeScreen:3]"/>
            </StackPanelWidget>
          </CanvasWidget>
        </BevelledButtonWidget>
        <CanvasWidget Size="4,0"/>
        <BevelledButtonWidget Name="GVOscilloscopeScreen.BottomButton" Size="188, 60">
          <CanvasWidget Name="BevelledButton.Canvas" Margin="0, 0" HorizontalAlignment="Near">
            <BevelledRectangleWidget Name="BevelledButton.Rectangle" CenterColor="112, 112, 112"/>
            <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Near" VerticalAlignment="Center" IsHitTestVisible="false">
              <LabelWidget Name="GVOscilloscopeScreen.BottomInput" VerticalAlignment="Center" Margin="10, 0" Text="[GVOscilloscopeScreen:4]"/>
            </StackPanelWidget>
          </CanvasWidget>
        </BevelledButtonWidget>
        <CanvasWidget Size="4,0"/>
        <BevelledButtonWidget Name="GVOscilloscopeScreen.LeftButton" Size="188, 60">
          <CanvasWidget Name="BevelledButton.Canvas" Margin="0, 0" HorizontalAlignment="Near">
            <BevelledRectangleWidget Name="BevelledButton.Rectangle" CenterColor="112, 112, 112"/>
            <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Near" VerticalAlignment="Center" IsHitTestVisible="false">
              <LabelWidget Name="GVOscilloscopeScreen.LeftInput" VerticalAlignment="Center" Margin="10, 0" Text="[GVOscilloscopeScreen:5]"/>
            </StackPanelWidget>
          </CanvasWidget>
        </BevelledButtonWidget>
        <CanvasWidget Size="4, 0"/>
        <BevelledButtonWidget Name="GVOscilloscopeScreen.Pause" Size="60, 60">
          <CanvasWidget Name="BevelledButton.Canvas" Margin="0, 0">
            <BevelledRectangleWidget Name="BevelledButton.Rectangle" CenterColor="50, 150, 35" BevelColor="50, 150, 35"/>
            <RectangleWidget Name="GVOscilloscopeScreen.PauseIcon" Size="40, 40" OutlineColor="0, 0, 0, 0" FillColor="255, 255, 255" HorizontalAlignment="Center" VerticalAlignment="Center"/>
          </CanvasWidget>
        </BevelledButtonWidget>
        <CanvasWidget Size="4, 0"/>
        <BevelledButtonWidget Name="GVOscilloscopeScreen.Step" Size="60, 60">
          <CanvasWidget Name="BevelledButton.Canvas" Margin="0, 0">
            <BevelledRectangleWidget Name="BevelledButton.Rectangle" CenterColor="50, 150, 35" BevelColor="50, 150, 35"/>
            <RectangleWidget Size="40, 40" Subtexture="{Textures/Gui/GVStep}" OutlineColor="0, 0, 0, 0" FillColor="255, 255, 255" HorizontalAlignment="Center" VerticalAlignment="Center"/>
          </CanvasWidget>
        </BevelledButtonWidget>
        <CanvasWidget Size="4, 0"/>
        <BevelledButtonWidget Name="GVOscilloscopeScreen.Jump" Size="60, 60">
          <CanvasWidget Name="BevelledButton.Canvas" Margin="0, 0">
            <BevelledRectangleWidget Name="BevelledButton.Rectangle" CenterColor="50, 150, 35" BevelColor="50, 150, 35"/>
            <RectangleWidget Size="40, 40" Subtexture="{Textures/Gui/GVJump}" OutlineColor="0, 0, 0, 0" FillColor="255, 255, 255" HorizontalAlignment="Center" VerticalAlignment="Center"/>
          </CanvasWidget>
        </BevelledButtonWidget>
      </StackPanelWidget>
      <CanvasWidget Name="GVOscilloscopeScreen.Canvas" Size="Infinity, Infinity" Margin="0, 4">
        <ScrollPanelWidget Name="GVOscilloscopeScreen.ScrollPanel" Direction="Vertical" ClampToBounds="true">
          <StackPanelWidget Direction="Vertical">
            <CanvasWidget Name="GVOscilloscopeScreen.Placeholder1" Size="1, 0"/>
            <StackPanelWidget Name="GVOscilloscopeScreen.ScrollStack" Direction="Vertical"/>
            <CanvasWidget Name="GVOscilloscopeScreen.Placeholder2" Size="1, 0"/>
          </StackPanelWidget>
        </ScrollPanelWidget>
      </CanvasWidget>
    </StackPanelWidget>
  </StackPanelWidget>
</Screen>