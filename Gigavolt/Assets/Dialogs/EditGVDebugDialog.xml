<CanvasWidget xmlns="runtime-namespace:Game">
  <CanvasWidget Size="720, 440" ClampToBounds="true" HorizontalAlignment="Center" VerticalAlignment="Center">
    <BevelledRectangleWidget Style="{Styles/DialogArea}"/>
    <StackPanelWidget Direction="Vertical" HorizontalAlignment="Center" VerticalAlignment="Near">
      <LabelWidget Font="{Fonts/Pericles}" Text="[EditGVDebugDialog:1]" HorizontalAlignment="Center" Margin="0, 15"/>
      <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Near">
        <StackPanelWidget Direction="Vertical" HorizontalAlignment="Center" VerticalAlignment="Near">
          <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 10">
            <LabelWidget Text="[EditGVDebugDialog:4]" VerticalAlignment="Center"/>
            <CanvasWidget Size="20, 0"/>
            <CanvasWidget Size="124, 34">
              <BevelledRectangleWidget Style="{Styles/Area}"/>
              <TextBoxWidget Name="EditGVDebugDialog.Speed" Size="120, 30" MaximumLength="11" FontScale="1" TextureLinearFilter="false" Margin="2, 2"/>
            </CanvasWidget>
          </StackPanelWidget>
          <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 10">
            <LabelWidget Text="[EditGVDebugDialog:8]" WordWrap="true" VerticalAlignment="Center"/>
            <CanvasWidget Size="20, 0"/>
            <CheckboxWidget Name="EditGVDebugDialog.DisplayVoltage" CheckboxSize="24, 24" IsAutoCheckingEnabled="false" Margin="0, 10" VerticalAlignment="Center"/>
          </StackPanelWidget>
          <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 10">
            <LabelWidget Text="[EditGVDebugDialog:7]" WordWrap="true" VerticalAlignment="Center"/>
            <CanvasWidget Size="20, 0"/>
            <CheckboxWidget Name="EditGVDebugDialog.PreventChunkFromBeingFree" CheckboxSize="24, 24" IsAutoCheckingEnabled="false" Margin="0, 10" VerticalAlignment="Center"/>
          </StackPanelWidget>
          <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 10">
            <LabelWidget Text="[EditGVDebugDialog:11]" WordWrap="true" VerticalAlignment="Center"/>
            <CanvasWidget Size="20, 0"/>
            <CheckboxWidget Name="EditGVDebugDialog.LoadChunkInAdvance" CheckboxSize="24, 24" IsAutoCheckingEnabled="false" Margin="0, 10" VerticalAlignment="Center"/>
          </StackPanelWidget>
        </StackPanelWidget>
        <CanvasWidget Size="30, 0"/>
        <StackPanelWidget Direction="Vertical" HorizontalAlignment="Center" VerticalAlignment="Near">
          <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 10">
            <LabelWidget Text="[EditGVDebugDialog:5]" VerticalAlignment="Center"/>
            <CanvasWidget Size="20, 0"/>
            <CheckboxWidget Name="EditGVDebugDialog.DisplayStepFloatingButtons" CheckboxSize="24, 24" IsAutoCheckingEnabled="false" Margin="0, 10" VerticalAlignment="Center"/>
          </StackPanelWidget>
          <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 10">
            <LabelWidget Text="[EditGVDebugDialog:6]" WordWrap="true" VerticalAlignment="Center"/>
            <CanvasWidget Size="20, 0"/>
            <CheckboxWidget Name="EditGVDebugDialog.KeyboardControl" CheckboxSize="24, 24" IsAutoCheckingEnabled="false" Margin="0, 10" VerticalAlignment="Center"/>
          </StackPanelWidget>
          <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 10">
            <LabelWidget Text="[EditGVDebugDialog:10]" WordWrap="true" VerticalAlignment="Center"/>
            <CanvasWidget Size="20, 0"/>
            <CheckboxWidget Name="EditGVDebugDialog.WheelPanelEnabled" CheckboxSize="24, 24" IsAutoCheckingEnabled="false" Margin="0, 10" VerticalAlignment="Center"/>
          </StackPanelWidget>
        </StackPanelWidget>
      </StackPanelWidget>
      <CanvasWidget Size="0, Infinity"/>
      <StackPanelWidget Direction="Horizontal" HorizontalAlignment="Center" Margin="0, 15">
        <BevelledButtonWidget Name="EditGVDebugDialog.OK" Style="{Styles/ButtonStyle_160x60}" Text="[EditGVDebugDialog:2]" Margin="20, 0"/>
        <BevelledButtonWidget Name="EditGVDebugDialog.Cancel" Style="{Styles/ButtonStyle_160x60}" Margin="20, 0" Text="[EditGVDebugDialog:3]"/>
      </StackPanelWidget>
    </StackPanelWidget>
    <BevelledButtonWidget Name="EditGVDebugDialog.Help" Size="48, 48" Margin="15, 15">
      <RectangleWidget Size="30, 30" Name="BevelledButton.Image" Subtexture="{Textures/Atlas/HelpTopicIcon}" OutlineColor="0, 0, 0, 0" FillColor="255, 255, 255" HorizontalAlignment="Center" VerticalAlignment="Center" IsVisible="true"/>
    </BevelledButtonWidget>
  </CanvasWidget>
</CanvasWidget>